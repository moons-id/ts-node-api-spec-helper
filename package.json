{
  "name": "api-docs-helper",
  "version": "1.0.0",
  "description": "Node.js script to generate API documentation paths from templates",
  "main": "dist/api-docs-generator.js",
  "type": "module",
  "bin": {
    "api-docs": "./dist/api-docs-generator.js",
    "api-docs:update": "./dist/api-docs-update.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/api-docs-generator.ts",
    "dev:update": "ts-node src/api-docs-update.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moons-id/ts-node-api-spec-helper.git"
  },
  "keywords": [
    "api",
    "documentation",
    "yaml",
    "openapi"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "js-yaml": "^4.1.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
